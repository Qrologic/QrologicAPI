@section Scripts{
    <script>
        $(document).ready(function () {
            menu.BindBreadCrumb(1);
        });
        $(".container").on('click', '.deleteBtn', function () {
            var val = this.id;
            Swal.fire({
                title: 'Are you sure?',
                text: "You won't be able to revert this!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, delete it!',
                allowOutsideClick: false
            }).then((result) => {
                if (result.value) {
                    $.ajax({
                        type: "POST",
                        url: 'Category/UpdateDelete',
                        data: { 'id': val },
                        success: function (data) {
                            if (data.IsSuccess) {
                                Swal.fire(
                                    'Deleted!',
                                    'Deleted Success .!!',
                                    'success'
                                )
                                loadGrid();
                            }
                        },
                        error: function (data) {
                            Toast.fire({
                                icon: 'error',
                                title: 'Error :' + data.errorMessage,
                            })
                        }

                    });
                }

            });
        });

    </script>
}
