@model QroApi.MODEL.MemberDetail
<div class="row">
    <div class="col-md-6 card-1">
        <div class="panel panel-default card-detail">
            <div class="panel-body">
                <div class="card-content secondary-header">
                    Basic information
                    <span class="pull-right text-primary edit-link" data-config-id="accountDetailsDisplayInfo">
                        @*<span data-toggle="modal" data-target="#Basic-information" onclick="menu.AdminMenuPopup(0);">Edit</span>*@
                    </span>
                </div>
                <div class="scroll-area-sm">
                    <div class="scrollbar-container ps">
                        <div class="card-content pb20">
                            <div></div>
                            <div class="col-md-12">
                                <span class="help-text light">
                                    Name: <span class="help-text">@Model.Name</span>
                                    @{ var iconClass = Model.Status == "Active" ? "fa-check-circle text-success" : "fa-times-circle text-danger";}
                                    <i class="fa @iconClass" aria-hidden="true"></i>
                                </span>
                            </div>
                            <div class="col-md-12">
                                <span class="help-text light">Member Id: <span class="help-text">@Model.MemberId</span></span>
                            </div>
                            <div class="col-md-12">
                                <span class="help-text light">DOJ: <span class="help-text">@Model.DOJ</span></span>
                            </div>

                            <div class="col-md-12">
                                <span class="help-text light">Aadhar Number: <span class="help-text">@Model.AadharNumber</span></span>
                            </div>
                            <div class="col-md-12">
                                <span class="help-text light">Pan Number: <span class="help-text">@Model.PanNumber</span></span>
                            </div>
                            <div class="col-md-12">
                                <span class="help-text light">Sponsor Id: <span class="help-text">@Model.SponsorId</span></span>
                            </div>
                            <div class="col-md-12">
                                <span class="help-text light">Sponsor Name: <span class="help-text">@Model.SponsorName</span></span>
                            </div>
                            <div class="col-md-12">
                                <span class="help-text light">Package: <span class="help-text">@Model.Package</span></span>
                            </div>

                        </div>
                        <div class="ps__rail-x" style="left: 0px; bottom: 0px;"><div class="ps__thumb-x" tabindex="0" style="left: 0px; width: 0px;"></div></div><div class="ps__rail-y" style="top: 0px; right: 0px; height: 200px;"><div class="ps__thumb-y" tabindex="0" style="top: 0px; height: 0px;"></div></div>
                    </div>
                </div>

            </div>
            <div class="panel-footer text-center load-more" style="display: none;">
                <span class="load-more-text more">
                    More
                    <i class="fa fa-angle-down"></i>
                </span>
                <span class="load-more-text less">
                    Less
                    <i class="fa fa-angle-up"></i>
                </span>
            </div>
        </div>
    </div>


    <div class="col-md-6" id="Contact">
        <div class="panel panel-default card-detail">
            <div class="panel-body">
                <div class="card-content secondary-header">
                    Contact Details
                </div>
                <div class="scroll-area-sm">
                    <div class="scrollbar-container ps">
                        <div class="card-content pb20">
                            <div></div>
                            <div class="col-md-12">
                                <span class="help-text light">Mobile: <span class="help-text">@Model.Mobile  </span></span>
                            </div>
                            <div class="col-md-12">
                                <span class="help-text light">Email:  <span class="help-text">@Model.Email</span></span>
                            </div>
                            <div class="col-md-12">
                                <span class="help-text light">Address: <span class="help-text">@Model.Address</span></span>
                            </div>
                            <div class="col-md-12">
                                <span class="help-text light">City: <span class="help-text">@Model.City</span></span>
                            </div>
                            <div class="col-md-12">
                                <span class="help-text light">State: <span class="help-text">@Model.State</span></span>
                            </div>


                        </div>
                        <div class="ps__rail-x" style="left: 0px; bottom: 0px;"><div class="ps__thumb-x" tabindex="0" style="left: 0px; width: 0px;"></div></div><div class="ps__rail-y" style="top: 0px; right: 0px; height: 200px;"><div class="ps__thumb-y" tabindex="0" style="top: 0px; height: 0px;"></div></div>
                    </div>
                </div>

            </div>
            @*<div class="panel-footer text-center load-more" id="contactBtn">
                    <span class="load-more-text more">
                        More
                        <i class="fa fa-angle-down"></i>
                    </span>
                    <span class="load-more-text less">
                        Less
                        <i class="fa fa-angle-up"></i>
                    </span>
                </div>*@
        </div>
    </div>


</div>